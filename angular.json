{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "MoiTruongNKPFrontEnd": {
      "projectType": "application",
      "schematics": {
       },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/MoiTruongNKPFrontEnd",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/styles.scss"    
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/nprogress/nprogress.js",
              "node_modules/easy-responsive-tabs/js/easyResponsiveTabs.js",
              "node_modules/minicart/dist/minicart.js",
              "node_modules/magnific-popup/dist/jquery.magnific-popup.js",
              "node_modules/mdbootstrap/js/popper.min.js",
              "node_modules/mdbootstrap/js/mdb.min.js",
              "node_modules/jcarousel/dist/jquery.jcarousel.min.js",
              "src/assets/js/custom.js",
              "src/assets/js/jquery.flexisel.js",
              "src/assets/js/jquery.wmuSlider.js",
              "src/assets/js/jquery.slicebox.js",
              "src/assets/js/move-top.js",
              "src/assets/js/jquery.flexslider.js",
              "src/assets/js/isotope.pkgd.min.js",
              
              "node_modules/alertifyjs/build/alertify.js",
              "node_modules/js-image-zoom/js-image-zoom.js"

              
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "MoiTruongNKPFrontEnd:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "MoiTruongNKPFrontEnd:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "MoiTruongNKPFrontEnd:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "MoiTruongNKPFrontEnd:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "MoiTruongNKPFrontEnd:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "MoiTruongNKPFrontEnd"
}